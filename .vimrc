"line numbers and other similar stuff"
set number
set cursorline
"syntax highlighting"
syntax on
"recognize file type"
filetype on
filetype plugin on
filetype indent on
set wildmenu
set cindent
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
"stops lines from wrapping"
set nowrap
"ignore case when searching"
set ignorecase
